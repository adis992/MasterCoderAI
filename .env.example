# API_KEY - Ključ za pristupanje eksternim API servisima (npr. OpenAI, HuggingFace, Google, DeepL, itd.)
# API_KEY - Key for accessing external API services (e.g. OpenAI, HuggingFace, Google, DeepL, etc.)
API_KEY=your_real_api_key_here

# DATABASE_URL - Putanja do baze podataka (SQLite, PostgreSQL, MySQL, itd.)
# DATABASE_URL - Database connection string (SQLite, PostgreSQL, MySQL, etc.)
DATABASE_URL=sqlite:///podaci.db

# DEBUG - Debug mod za razvoj
# DEBUG - Debug mode for development
DEBUG=True

# SECRET_KEY - Tajni ključ za aplikaciju (FastAPI, JWT, itd.)
# SECRET_KEY - Secret key for the app (FastAPI, JWT, etc.)
SECRET_KEY=your_secret_key_here

# LOG_LEVEL - Nivo logovanja (info, warning, error)
# LOG_LEVEL - Logging level (info, warning, error)
LOG_LEVEL=info

# PROMETHEUS_PORT - Port za Prometheus monitoring
# PROMETHEUS_PORT - Port for Prometheus monitoring
PROMETHEUS_PORT=9090

# GRAFANA_PORT - Port za Grafana dashboard
# GRAFANA_PORT - Port for Grafana dashboard
GRAFANA_PORT=3000

# MODEL_PATH - Putanja do GGUF modela (npr. Llama.cpp, OpenHermes, Mistral, Mixtral, WizardLM, Phi, Gemma, Qwen, Yi, DeepSeek, Solar, StableLM, TinyLlama, Starling, OpenChat, NeuralBeagle, MythoMax, WizardCoder, CodeUp, StableCode, DeepSeekCoder, LlamaCode, Phind, QwenCode, StarCoder, Octocoder, Replit, Magicoder, CodeGemma, itd.)
# MODEL_PATH - Path to GGUF model (see above for supported models)
MODEL_PATH=modeli/gguf-model.bin

# DVC_REMOTE_URL - URL za DVC remote storage (npr. S3, GCS, Azure, Minio, itd.)
# DVC_REMOTE_URL - DVC remote storage URL (e.g. S3, GCS, Azure, Minio, etc.)
DVC_REMOTE_URL=

# DOCKER_TOKEN - Token za autentikaciju na Docker registry (umjesto username/password)
# DOCKER_TOKEN - Token for Docker registry authentication (instead of username/password)
DOCKER_TOKEN=your-docker-token-here

# DOCKER_REGISTRY - Docker registry URL
# DOCKER_REGISTRY - Docker registry URL
DOCKER_REGISTRY=your-docker-registry-url

# DOCKER_USERNAME - Docker korisničko ime
# DOCKER_USERNAME - Docker username
# DOCKER_PASSWORD - Docker lozinka
# DOCKER_PASSWORD - Docker password
# (Ako koristiš DOCKER_TOKEN, DOCKER_USERNAME i DOCKER_PASSWORD nisu potrebni)

# TERRAFORM_CLOUD_TOKEN - Token za Terraform Cloud
# TERRAFORM_CLOUD_TOKEN - Terraform Cloud token
TERRAFORM_CLOUD_TOKEN=your-terraform-token

# Example environment variables
API_KEY=
DATABASE_URL=
DEBUG=False
MODEL_PATH=modeli/gguf-model.bin
API_TIMEOUT=30
LOG_LEVEL=info
MAX_TOKENS=1024
TEMPERATURE=0.7